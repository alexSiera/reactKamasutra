{
  "homepage": "https://alexsiera.github.io/reactSocialNetwork/",
  "name": "social-network-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "pretty-quick": "pretty-quick",
    "typescript": "tsc",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:ts": "eslint './src/**/*.{ts, tsx}'",
    "prettier": "prettier --single-quote --use-tabs --trailing-comma es5 --write \"./src/**/js/*.js\"",
    "prettier:ts": "prettier-eslint --list-different $PWD/'src/**/*.{ts, tsx}'; prettier-eslint --write ${PWD}/'src/**/*.{ts, tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.11",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-form": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-jest-enzyme": "^7.1.2",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-pretty-imports": "^1.1.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^16.14.0",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
